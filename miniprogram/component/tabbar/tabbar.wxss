.tabbar_box{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 98rpx;
    box-shadow: 0 0 2px rgba(0, 0, 0, 0.1);
}

.middle-wrapper{
  position: absolute;
  right: 310rpx;
  bottom: 0;
  background-color: #fff;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border-top: 2rpx solid #f2f2f3;
}

.tabbar_nav{
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    height: 100%;
    position: relative;
}
.tabbar_icon{
    width: 56rpx;
    height: 56rpx;
}
.special-wrapper{
  position: absolute;
  left: 47rpx;
  top:-86rpx;
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  border-top: 1rpx solid rgb(79, 115, 156);
  background-color: #fff;
  text-align: center;
  box-sizing: border-box;
  padding: 6rpx;
  box-shadow:0px 0px 5px 1px rgb(79, 115, 156);
}
.special-wrapper .tabbar_icon{
    width: 152rpx;
    height: 152rpx;
}
.special-text-wrapper{
  width: 56rpx;
  height: 56rpx;
}