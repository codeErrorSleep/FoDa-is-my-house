/* 快递选项卡 */
 .select_field{
  display: flex;
  width: 100%;
  line-height: 80rpx;
  background-color: white;
  justify-content: space-around;
}
.select_field .select_box{
  flex: auto;
  text-align: center;
  /* font-weight: bold; */
}
.select_text{
  font-size: 30rpx; 
}
.select_text.active{
  color: #1296DB;
}
.select_img{
  font-size: 40rpx;
  transition:transform 0.3s;
}
.select_img.active{
  color: #1296DB;
  transform:rotate(180deg); 
}



.select_oneCol{
  position: absolute;
  z-index: 3;
  width: 100%;
  overflow-y: scroll;
  line-height:100rpx;
  background: white;
}
.select_threeCol{
  position: absolute;
  width:100%;
  height: 450rpx;
  z-index: 3;
}
.select_threeCol .col_detail{
  overflow-y: scroll;
  float: left;
  width: 33.33%;
  height: 450rpx;
  line-height: 80rpx;
  box-sizing: border-box; 
  font-size: 14px;
}
.current1{
  color: #1296DB;
  background: rgb(248, 252, 255);
}
.current2{
  color: #1296DB;
  background: rgb(239, 249, 255);
}
.mask{
  position: absolute;
  z-index: 2;
  background: #c5c5c5; opacity:0.5;
  width: 100%;
  height: 100%;
}

