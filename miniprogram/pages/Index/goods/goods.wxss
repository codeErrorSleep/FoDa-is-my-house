 .swiper{
   display: flex; 
   width: 100%; 
   flex-direction: row; 
   line-height: 80rpx; 
   position: fixed;
   max-height: 100%;
   height: 100%;
 }
 
 
 
 .container {
   padding: 0 12.5rpx 0 12.5rpx;
   width:750rpx;
   display: flex;
   flex-direction:row;
   flex-wrap:wrap;
   justify-content:space-between;
 }
 
 
 .item_each{
   margin: 7.5rpx 5rpx 7.5rpx 5rpx;
   display: inline-block;
   width: 352.5rpx;
   border-radius: 3%;
   background-color: #fff;
 }
 
 .item_cell{
   margin: 0 20rpx 0 20rpx;
   display: flex;
   align-items: center;
   justify-content: space-between;
 }
 
 .item_text {
   overflow: hidden;
   text-overflow: ellipsis;
   display: -webkit-box;
   -webkit-line-clamp: 1;
   -webkit-box-orient: vertical;
   align-content: center;
   color:black;
 }
 
 .item_price{
   display:inline-block;
   font-size: 30rpx;
   font-weight: bold;
   color:#FF4F64;
 
 }
 
 .item_price_before{
   display:inline-block;
   flex:1rpx;
   font-size: 25rpx;
   text-decoration:line-through;
   color:#B2B2B2;
 }
 
 
 
