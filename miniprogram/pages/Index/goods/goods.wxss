.navbar{ 
  display: flex; 
  width: 100%; 
  flex-direction: row; 
  line-height: 80rpx; 
 } 
 .navbar .item{ 
  flex: auto; 
  font-size: 30rpx; 
  text-align: center; 
  background: #fff; 
  /* font-weight: bold;  */
 } 
 .navbar .item.active{ 
  color: #1296DB; 
  position: relative; 
 } 
 .navbar .item.active::after{ 
  content: "";
  display: block;
  position: absolute;
  margin-left: auto;
  margin-right:auto;
  width: 50%;
  height: 4rpx; 
  bottom: 0;
  left: 0;
  right: 0;
  background: #1296DB;
 }
 .categories{ 
  display: flex; 
  width: 100%; 
  flex-direction: row; 
  line-height: 80rpx; 
 } 
 .categories .item{ 
  flex: auto; 
  font-size: 30rpx; 
